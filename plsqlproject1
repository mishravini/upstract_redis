CREATE TABLE employees (
    emp_id    NUMBER PRIMARY KEY,
    emp_name  VARCHAR2(100),
    salary    NUMBER,
    dept_id   NUMBER
);
    INSERT INTO employees VALUES (101, 'Vini', 60000, 10);
